<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/rlMainPage"

    tools:ignore="ContentDescription,RtlHardcoded,RelativeOverlap,UselessParent,RtlSymmetry,SpUsage,UnusedAttribute"
    android:background="@android:color/transparent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
<LinearLayout
    android:id="@+id/llMain"
    android:layout_alignParentBottom="true"
    android:background="@drawable/bg_main"
    android:layout_width="match_parent"
    android:layout_height="@dimen/height_activity_failed_result"
    android:orientation="vertical"
    tools:context=".activities.selectionportal.SelectionActivity">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/m_8">




        <ImageView
            android:layout_centerInParent="true"
            android:layout_gravity="center"
            android:layout_width="@dimen/w_48"
            android:layout_height="@dimen/h_48"
            android:src="@drawable/ic_failed" />





    </RelativeLayout>

    <TextView
        android:layout_marginTop="@dimen/m_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:textColor="@color/failed"
        android:text="@string/failed"
        android:textSize="@dimen/t_15" />


    <RelativeLayout
        android:id="@+id/rlTop"
        android:layout_marginTop="@dimen/m_8"
        android:background="@color/main_top"
        android:layout_marginLeft="@dimen/m_24"
        android:layout_marginRight="@dimen/m_24"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">



        <RelativeLayout

            android:layout_marginTop="@dimen/m_24"
            android:layout_marginLeft="@dimen/m_16"
            android:layout_marginRight="@dimen/m_16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">




            <TextView
                android:textColor="@color/text_font"
                android:id="@+id/txtTerminalLabel"
                android:layout_alignParentRight="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/terminal_name"
                android:textSize="@dimen/t_14"
                />




            <TextView
                android:textColor="@color/text_font"
                android:id="@+id/txtTerminalName"
                android:layout_alignParentLeft="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="هیلایی"
                android:textSize="@dimen/t_14" />


        </RelativeLayout>

            <View
                android:id="@+id/viewLine1"
                android:layout_width="match_parent"
                android:layout_height="@dimen/h_1"
                android:layout_marginBottom="@dimen/m_8"
                android:layout_marginTop="@dimen/m_8"
                android:background="@color/line_success" />

            <RelativeLayout

                android:layout_marginLeft="@dimen/m_16"
                android:layout_marginRight="@dimen/m_16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">




                <TextView
                    android:textColor="@color/text_font"
                    android:id="@+id/txtTerminalNumberLabel"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/terminal_number"
                    android:textSize="@dimen/t_14" />


                <TextView
                    android:textColor="@color/text_font"
                    android:id="@+id/txtTerminalNumber"
                    android:layout_alignParentLeft="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    tools:text="10101001"
                    android:textSize="@dimen/t_14" />


            </RelativeLayout>

            <View
                android:id="@+id/viewLine2"
                android:layout_width="match_parent"
                android:layout_height="@dimen/h_1"
                android:layout_marginBottom="@dimen/m_8"
                android:layout_marginTop="@dimen/m_8"
                android:background="@color/line_success" />

            <RelativeLayout
                android:layout_marginLeft="@dimen/m_16"
                android:layout_marginRight="@dimen/m_16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">




                <TextView
                    android:textColor="@color/text_font"
                    android:id="@+id/txtPriceLabel"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/price"
                    android:textSize="@dimen/t_14" />




                <TextView
                    android:textColor="@color/text_font"
                    android:id="@+id/txtPrice"
                    android:layout_alignParentLeft="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    tools:text="10101001"
                    android:textSize="@dimen/t_14" />


            </RelativeLayout>

            <View
                android:id="@+id/viewLine3"
                android:layout_width="match_parent"
                android:layout_height="@dimen/h_1"
                android:layout_marginBottom="@dimen/m_8"
                android:layout_marginTop="@dimen/m_8"
                android:background="@color/line_success" />

            <RelativeLayout
                android:layout_marginLeft="@dimen/m_16"
                android:layout_marginRight="@dimen/m_16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">




                <TextView
                    android:textColor="@color/text_font"
                    android:id="@+id/txtTransactionLabel"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/transaction"
                    android:textSize="@dimen/t_14" />




                <TextView
                    android:textColor="@color/text_font"
                    android:id="@+id/txtTransaction"
                    android:layout_alignParentLeft="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    tools:text="10101001"
                    android:textSize="@dimen/t_14" />


            </RelativeLayout>
            <View
                android:id="@+id/viewLine4"
                android:layout_width="match_parent"
                android:layout_height="@dimen/h_1"
                android:layout_marginBottom="@dimen/m_8"
                android:layout_marginTop="@dimen/m_8"
                android:background="@color/line_success" />

            <RelativeLayout
                android:layout_marginBottom="@dimen/m_24"
                android:layout_marginLeft="@dimen/m_16"
                android:layout_marginRight="@dimen/m_16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">




                <TextView
                    android:textColor="@color/text_font"
                    android:id="@+id/txtBankPortNumberLabel"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/bank_port_number"
                    android:textSize="@dimen/t_14" />



                <TextView
                    android:textColor="@color/text_font"
                    android:id="@+id/txtBankPortNumber"
                    android:layout_alignParentLeft="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    tools:text="10101001"
                    android:textSize="@dimen/t_14" />


            </RelativeLayout>
        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/rlBackBtn"
        android:layout_marginLeft="@dimen/m_18"
        android:layout_marginRight="@dimen/m_18"
        android:layout_marginTop="@dimen/m_8"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_alignLeft="@+id/llBtnFailed"
            android:layout_alignTop="@+id/llBtnFailed"
            android:layout_alignRight="@+id/llBtnFailed"
            android:layout_alignBottom="@+id/llBtnFailed"
            android:id="@+id/imgBtnFailed"
            android:gravity="center"
            android:scaleType="fitXY"
            android:src="@drawable/bg_failed"
            android:layout_width="match_parent"
            android:layout_height="@dimen/h_0"/>

        <LinearLayout
            android:id="@+id/llBtnFailed"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/txtBtnFailed"
                android:padding="@dimen/p_4"
                android:textSize="@dimen/t_19"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/button_font"
                android:text="@string/back"
                />

        </LinearLayout>

    </RelativeLayout>


</LinearLayout>

</RelativeLayout>